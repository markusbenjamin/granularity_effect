Cell[BoxData[
 RowBox[{
  RowBox[{"dotRatio", "=", "0.5"}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"res", "=", 
   RowBox[{"Parallelize", "[", 
    RowBox[{"Table", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Module", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "image", ",", "posList", ",", "fillingRatio", ",", "fitParam"}], 
         "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"image", "=", 
          RowBox[{"ConstantArray", "[", 
           RowBox[{"0", ",", 
            RowBox[{"Round", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"dotSize", "/", "dotRatio"}], ",", 
               RowBox[{"29", " ", 
                RowBox[{"dotSize", "/", "dotRatio"}]}]}], "}"}], "]"}]}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"posList", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"{", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"RandomInteger", "[", 
               RowBox[{
                RowBox[{"Dimensions", "[", "image", "]"}], "[", 
                RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{"RandomInteger", "[", 
               RowBox[{
                RowBox[{"Dimensions", "[", "image", "]"}], "[", 
                RowBox[{"[", "2", "]"}], "]"}], "]"}]}], 
             "\[IndentingNewLine]", "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"n", ",", "1", ",", "900"}], "}"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"fillingRatio", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"{", "\[IndentingNewLine]", 
             RowBox[{"n", ",", "\[IndentingNewLine]", 
              RowBox[{"N", "[", 
               RowBox[{
                RowBox[{"Length", "[", 
                 RowBox[{"Select", "[", "\[IndentingNewLine]", 
                  RowBox[{
                   RowBox[{"DeleteDuplicates", "[", 
                    RowBox[{"Flatten", "[", 
                    RowBox[{
                    RowBox[{"Table", "[", 
                    RowBox[{
                    RowBox[{"Map", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"pos", "+", "#"}], "&"}], ",", "dotPoints"}], 
                    "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"pos", ",", 
                    RowBox[{"posList", "[", 
                    RowBox[{"[", 
                    RowBox[{"1", ";;", "n"}], "]"}], "]"}]}], "}"}]}], "]"}], 
                    ",", "1"}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
                   RowBox[{
                    RowBox[{
                    RowBox[{"Between", "[", 
                    RowBox[{
                    RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"1", ",", 
                    RowBox[{
                    RowBox[{"Dimensions", "[", "image", "]"}], "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "}"}]}], "]"}], "&&", 
                    "\[IndentingNewLine]", 
                    RowBox[{"Between", "[", 
                    RowBox[{
                    RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"1", ",", 
                    RowBox[{
                    RowBox[{"Dimensions", "[", "image", "]"}], "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}], "]"}]}], 
                    "\[IndentingNewLine]", "&"}]}], "]"}], "]"}], "/", 
                RowBox[{"ProductAcross", "[", 
                 RowBox[{"Dimensions", "[", "image", "]"}], "]"}]}], "]"}]}], 
             "\[IndentingNewLine]", "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"n", ",", "1", ",", "900", ",", "25"}], "}"}]}], "]"}]}],
          ";", "\[IndentingNewLine]", 
         RowBox[{"fitParam", "=", 
          RowBox[{"Mean", "[", 
           RowBox[{"Flatten", "[", 
            RowBox[{"Select", "[", 
             RowBox[{
              RowBox[{"Map", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"a", "/.", 
                  RowBox[{"Quiet", "[", 
                   RowBox[{"Solve", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"2", "-", 
                    RowBox[{"(", 
                    RowBox[{"1", "+", 
                    RowBox[{"Exp", "[", 
                    RowBox[{
                    RowBox[{"-", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "/", "a"}], "]"}]}], 
                    ")"}]}], "==", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], ",", "a"}], "]"}], 
                   "]"}]}], "&"}], ",", "fillingRatio"}], "]"}], ",", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"Length", "[", "#", "]"}], "==", "1"}], "&&", 
                RowBox[{"NumberQ", "[", 
                 RowBox[{"#", "[", 
                  RowBox[{"[", "1", "]"}], "]"}], "]"}]}], "&"}]}], "]"}], 
            "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"dotRatio", ",", "fitParam", ",", "fillingRatio"}], 
          "}"}]}]}], "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", ",", 
      RowBox[{"{", 
       RowBox[{"dotRatio", ",", "0.01", ",", "0.3", ",", "0.01"}], "}"}]}], 
     "]"}], "]"}]}], ";"}]], "Input",
