Cell[BoxData[{
 RowBox[{
  RowBox[{"dotSize", "=", "21"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dotPoints", "=", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Table", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"EuclideanDistance", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"0", ",", "0"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "j"}], "}"}]}], "]"}], "<", 
            RowBox[{"dotSize", "/", "2"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"i", ",", "j"}], "}"}], ",", "\[IndentingNewLine]", 
           "Nothing"}], "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", 
         ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", 
           RowBox[{"Floor", "[", 
            RowBox[{
             RowBox[{"-", "dotSize"}], "/", "2"}], "]"}], ",", 
           RowBox[{"Ceiling", "[", 
            RowBox[{"dotSize", "/", "2"}], "]"}]}], "}"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Floor", "[", 
          RowBox[{
           RowBox[{"-", "dotSize"}], "/", "2"}], "]"}], ",", 
         RowBox[{"Ceiling", "[", 
          RowBox[{"dotSize", "/", "2"}], "]"}]}], "}"}]}], "]"}], ",", "1"}], 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.8708722813654933`*^9, 3.8708724299159784`*^9}, {
